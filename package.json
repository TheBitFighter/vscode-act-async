{
  "name": "act-async",
  "displayName": "ACT Async",
  "description": "Language support for the Yale AVLSI ACT toolchain.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "act",
      "aliases": ["ACT", "act"],
      "extensions": ["act"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "act",
      "scopeName": "source.act",
      "path": "./syntaxes/act.tmLanguage.json"
    }]
  }
}
